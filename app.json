{
  "expo": {
    "name": "ecommerce-store",
    "slug": "ecommerce-store",
    "version": "1.0.0",
    "owner": "hunter47",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.hunter47.ecommercestore",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hunter47.ecommercestore"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "android": {
      "package": "com.hunter47.ecommercestore",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
          "apiKey": "AIzaSyD_VVvZR4cX8RWObY-28HijR7p7tEf8Etc",
          "certificateHash": "8db61544f6c4b8211d98d1575440acf8c1b42489"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b695f58a-1d8d-4bf6-bc92-ccae46732b47"
      }
    }
  }
}
